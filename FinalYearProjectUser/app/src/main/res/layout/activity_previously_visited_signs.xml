<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.shahzademambaccus.finalyearprojectuser.PreviouslyVisitedSigns">

    <include
        android:id="@+id/Tool_Bar_Previously_Visited"
        layout="@layout/tool_bar"
        />

    <GridView
        android:id="@+id/Previously_Visited_Grid"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:numColumns="auto_fit"
        android:horizontalSpacing="12dp"
        android:columnWidth="120dp"
        android:stretchMode="spacingWidthUniform"
        android:layout_below="@+id/Tool_Bar_Previously_Visited" />

</RelativeLayout>
